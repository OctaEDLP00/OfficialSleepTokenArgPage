    "eslint": "9.26.0",
    "eslint-config-love": "119.0.0",
    "eslint-config-prettier": "10.1.5",
    "eslint-plugin-astro": "1.3.1",
    "eslint-plugin-jsx-a11y": "6.10.2",
    "globals": "16.1.0",
    "prettier": "3.5.3",
